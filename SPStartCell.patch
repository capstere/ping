+function Get-ConfigFlag {
+    param(
+        [Parameter(Mandatory=$true)][string]$Name,
+        [bool]$Default = $false,
+        [object]$ConfigOverride
+    )
+    try {
+        return [bool](Get-ConfigValue -Name $Name -Default $Default -ConfigOverride $ConfigOverride)
+    } catch {
+        return $Default
+    }
